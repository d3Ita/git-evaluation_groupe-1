#!/usr/bin/env python3
"""Minitrice - Calculatrice arithmétique élémentaire."""

import sys


def main():
    try:
        while True:
            print("> ", end="", flush=True)
            line = input()
            print(line)
    except EOFError:
        print()
        print("Fin des calculs :)")

    sys.exit(0)


if __name__ == "__main__":
    main()
